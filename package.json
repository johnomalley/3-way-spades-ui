{
  "name": "3-way-spades-ui",
  "version": "1.0.0",
  "description": "web UI for 3 way spades",
  "main": "index.js",
  "scripts": {
    "dev": "npm start",
    "start": "parcel -p 3002 index.html",
    "clean": "rm -rf dist",
    "build": "NODE_ENV=production npm run clean && parcel build index.html",
    "deploy:html": "aws s3 cp dist/index.html s3://${UI_S3_BUCKET}/index.html --cache-control 'max-age=0, must-revalidate'",
    "deploy:sync": "aws s3 sync --delete --exclude index.html dist s3://${UI_S3_BUCKET}",
    "deploy:only": "npm run deploy:html && npm run deploy:sync",
    "deploy": "npm run build && npm run deploy:only",
    "lint": "standardx --fix \"src/**/*.ts\" \"src/**/*.tsx\"",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/johnomalley/3-way-spades-ui.git"
  },
  "keywords": [
    "react",
    "bulma",
    "spades"
  ],
  "author": "John O'Malley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnomalley/3-way-spades-ui/issues"
  },
  "homepage": "https://github.com/johnomalley/3-way-spades-ui#readme",
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    },
    "overrides": [
      {
        "files": [
          "**/test/**/*.ts"
        ],
        "globals": {
          "jest": true,
          "describe": true,
          "beforeEach": true,
          "afterEach": true,
          "it": true,
          "expect": true
        }
      }
    ]
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "devDependencies": {
    "@babel/polyfill": "~7.12.1",
    "@fortawesome/fontawesome-free": "~5.15.3",
    "@types/classnames": "~2.2.11",
    "@types/enzyme": "~3.10.8",
    "@types/history": "~4.7.8",
    "@types/lodash": "~4.14.168",
    "@types/react": "~16.9.56",
    "@types/react-dom": "~16.9.12",
    "@types/react-redux": "~7.1.16",
    "@types/react-router-dom": "~5.1.7",
    "@types/redux-logger": "~3.0.8",
    "@types/superagent": "~4.1.10",
    "@typescript-eslint/eslint-plugin": "~4.20.0",
    "@typescript-eslint/parser": "~4.20.0",
    "browserslist": "~4.16.3",
    "bulma": "~0.9.2",
    "classnames": "~2.2.6",
    "connected-react-router": "~6.9.1",
    "enzyme": "~3.11.0",
    "history": "~4.10.1",
    "jest": "~26.6.3",
    "lodash": "~4.17.21",
    "moment": "~2.29.1",
    "nodemon": "~2.0.7",
    "parcel-bundler": "~1.12.5",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "react-redux": "~7.2.3",
    "react-router-dom": "~5.2.0",
    "redux": "~4.0.5",
    "redux-logger": "~3.0.6",
    "redux-saga": "~1.1.3",
    "sass": "~1.32.8",
    "standardx": "~7.0.0",
    "superagent": "~6.1.0",
    "ts-jest": "~26.5.4",
    "typescript": "~4.2.3"
  }
}
