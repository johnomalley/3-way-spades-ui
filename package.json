{
  "name": "3-way-spades-ui",
  "version": "1.0.0",
  "description": "web UI for 3 way spades",
  "main": "index.js",
  "scripts": {
    "dev": "npm start",
    "start": "parcel -p 3002 index.html",
    "lint": "standardx --fix \"src/**/*.ts\" \"src/**/*.tsx\"",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/johnomalley/3-way-spades-ui.git"
  },
  "keywords": [
    "react",
    "bulma",
    "spades"
  ],
  "author": "John O'Malley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnomalley/3-way-spades-ui/issues"
  },
  "homepage": "https://github.com/johnomalley/3-way-spades-ui#readme",
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    },
    "overrides": [
      {
        "files": [
          "**/test/**/*.ts"
        ],
        "globals": {
          "jest": true,
          "describe": true,
          "beforeEach": true,
          "afterEach": true,
          "it": true,
          "expect": true
        }
      }
    ]
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "devDependencies": {
    "@babel/polyfill": "~7.8.7",
    "@fortawesome/fontawesome-free": "~5.13.0",
    "@types/classnames": "~2.2.10",
    "@types/enzyme": "~3.10.5",
    "@types/history": "~4.7.5",
    "@types/lodash": "~4.14.150",
    "@types/react": "~16.9.34",
    "@types/react-dom": "~16.9.7",
    "@types/react-redux": "~7.1.8",
    "@types/react-router-dom": "~5.1.5",
    "@types/redux-logger": "~3.0.7",
    "@types/superagent": "~4.1.7",
    "@typescript-eslint/eslint-plugin": "~2.31.0",
    "@typescript-eslint/parser": "~2.31.0",
    "bulma": "~0.8.2",
    "classnames": "~2.2.6",
    "connected-react-router": "~6.8.0",
    "enzyme": "~3.11.0",
    "history": "~4.10.1",
    "jest": "~26.0.1",
    "lodash": "~4.17.19",
    "moment": "~2.25.3",
    "nodemon": "~2.0.3",
    "parcel-bundler": "~1.12.4",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-redux": "~7.2.0",
    "react-router-dom": "~5.1.2",
    "redux": "~4.0.5",
    "redux-logger": "~3.0.6",
    "redux-saga": "~1.1.3",
    "sass": "~1.26.5",
    "standardx": "~5.0.0",
    "superagent": "~5.2.2",
    "ts-jest": "~25.5.0",
    "typescript": "~3.8.3"
  }
}
